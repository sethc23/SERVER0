[core]
        autocrlf = input
        repositoryformatversion = 0
        filemode = true
        bare = false
        logallrefupdates = true
[diff]
        tool = ext_diff
[difftool "ext_diff"]
	cmd="/usr/bin/colordiff \
--side-by-side \
--suppress-common-lines \
--ignore-all-space \
--minimal \
--suppress-blank-empty \
--width=280 \
\"$LOCAL\" \"$REMOTE\" | less "


[difftool]
	prompt = false
[add]
        ignore-errors = true
[push]
	default = upstream
[color]
        ui = auto
[color "branch"]
        current = yellow bold
        local = green bold
        remote = cyan bold
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
        whitespace = red reverse
[color "status"]
        added = green bold
        changed = yellow bold
        untracked = red bold

